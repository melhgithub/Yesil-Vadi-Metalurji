@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">

<head>
    @await Html.PartialAsync("_ScriptImports")
    <title>Hata - Yeşilvadi Metalurji</title>
    <!-- Meta tag Keywords -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8" />
    <script>
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <!-- //Meta tag Keywords -->
    <!--/Style-CSS -->
    <link rel="stylesheet" href="~/weberror/css/style.css" type="text/css" media="all" />
    <!--//Style-CSS -->
    <!-- font-awesome-icons -->
    <link href="~/weberror/css/font-awesome.css" rel="stylesheet">
    <!-- //font-awesome-icons -->
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- //Bootstrap CSS -->
</head>

<body>
    <section class="w3l-error-61-404">
        <!-- /error-61-section -->
        <div class="error-61-mian" style="background: url('/Resimler/errorpage.jpg') no-repeat bottom;">>
            <div class="wrapper">
                <div class="errors-16-top">
                    <div class="errors-16-info">
                        <h3>404 </h3>
                        <p>Aranılan Sayfa Bulunamadı!</p>
                        <a href="/Home/" class="btn btn-primary">Ana Sayfa</a>
                    </div>
                </div>
                <div class="copyright-footer">
                    <div class="copy-right">
                        <p>
                            © 2023 Yeşilvadi Metalurji Hata Sayfası. Tüm hakları saklıdır.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- //error-61-section -->
</body>

</html>
